<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elasticbeanstalk.laciecool.first" >

    <!-- must be declared in the order of activity, service, receiver and provider -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SurveyQnsDisplayActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.elasticbeanstalk.laciecool.first.MainActivity" />
        </activity>

        <service
            android:name=".Keylogger"
            android:enabled="true"
            android:exported="true" >
        </service>

        <activity
            android:name=".IntroductionActivity"
            android:label="@string/title_activity_introduction" >
        </activity>
        <activity
            android:name=".IntroductionQnsActivity"
            android:label="IntroductionQnsActivity" >
        </activity>
        <activity
            android:name=".EndActivity"
            android:label="@string/title_activity_end" >
        </activity>
        <activity
            android:name=".PIActivity"
            android:label="@string/title_activity_pi" >
        </activity>
        <activity
            android:name=".ConsentActivity"
            android:label="@string/title_activity_consent" >
        </activity>
        <activity
            android:name=".RandomTwoActivity"
            android:label="@string/title_activity_random_two" >
        </activity>
    </application>

</manifest>
